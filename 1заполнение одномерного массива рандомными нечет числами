int[] arr = new int[50];
            Random rnd = new Random();
            for (int i = 0; i < arr.Length; i++)
            {
                arr[i] = rnd.Next(1, 50) *2 + 1;

                Console.WriteLine(arr[i]);
            }
            
            Console.ReadKey();
